.select_box{
 /* background: #fff; */
 width: 90%;
 margin: 10rpx auto;
 margin-top:-30rpx;
 /* position: relative; */
}
.select{
 box-sizing: border-box;
 width: 100%;
 height: 120rpx;
 border:1px solid #efefef;
 border-radius: 8rpx;
 display: flex;
 align-items: center;
 padding: 0 20rpx;
}
.select_text{
 font-size: 30rpx;
 flex: 1;
}
.select_img{
 width: 40rpx;
 height: 40rpx;
 display: block;
}
.select_img_rotate{
 transform:rotate(180deg); 
}
.textdiv{
  background: #ffffff;
height: 280rpx;
padding: 15rpx 20rpx;

}
.option_box{
 position: absolute;
 top: 245rpx;
 width: 90%;
 /* border-left:1px solid #efefef;
 border-right:1px solid #efefef;
 border-bottom:1px solid #efefef; */
 box-sizing: border-box;
 height: 0;
 overflow-y: auto;
 padding-left: 15px;
 /* background: #E9E9E9; */
 background:#ffffff;
 transition: height 0.3s;
 z-index:100;
 border-radius: 4px;
}
.option{
 display: block;
 line-height: 40rpx;
 font-size: 30rpx;
 border-bottom: 1px solid #efefef;
 padding: 10rpx 20rpx;
}