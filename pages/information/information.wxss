page{
  width:100%;
  height:100%;
  background-color: #F7F7F7;
  /* overflow: hidden;
  position: flex; */
  position:relative;
}

.reg_form{
width: 100%;
position: absolute;
/* padding-top: 50rpx; */
display: block;
top: 10vh;
height: 88vh;
background: #f3f3f3;
}
.background-image{
  width: 100%;
  /* height: 535px; */
  /* float: left;
  position: absolute; */
  z-index: 0;
  background-size: cover; 
}
.main_back{
  position: absolute;
  left: 20rpx;
  font-size: 36rpx;
  top: 110rpx;
}
.bg-gradual-block{
  background-color: #000000;
  color: #FFF;
}
.cu-form-group+.cu-form-group {
  border-top:none;
}
.reg_btn{
  position: absolute;
  width:100vw;
  margin-top:30px;
}
.reg_btn_default{
  width: 86%;
  background-color: #424242;
  color:#FFF;
  border-radius: 30px;
  font-weight: bold;
}
button[disabled]{
  background-color: #9B9B9B !important;
  color:#FFF !important;
  border:none;
}
.titleborder{
  background-color: #ffffff;
  width:100%;
  padding:10rpx 0;
  height: 140px;
}
.radio-group{
  width:100%;
}
.radio{
  display:block;
  text-align: center;
  height:50rpx;
  font-size:30rpx;
  border-radius: 20rpx;
  margin-bottom: 10rpx;
  line-height:50rpx;
  /* background:#ffffff; */
  position: relative;
}
radio{
  position: absolute;
  left:10%;
}
 label > text{
   left:22%;
   position:absolute;
 }

 ::placeholder{
   font-size:36rpx;
 }
 .scroll-view-y{
    height: 75vh;
    position: absolute;
    top: 25vh;
    background: #fff;
 }
.grid.grid-square>view>text[class*="cuIcon-"] {
	font-size: 52rpx;
	position: absolute;
	color: var(--grey);
	margin: auto;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	display: flex;
	justify-content: center;
	align-items: center;
	flex-direction: column;
}
.cuIcon-cameraadd:before {
content: "\e724";
}
.dscare{
  position: absolute;
  /* margin-top: -25px; */
  color: gray;
}
